					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"onpremiseadvetureLT_properties_typeProperties_server": {
						"value": "[parameters('onpremiseadvetureLT_properties_typeProperties_server')]"
					},
					"onpremiseadvetureLT_properties_typeProperties_database": {
						"value": "[parameters('onpremiseadvetureLT_properties_typeProperties_database')]"
					},
					"onpremiseadvetureLT_properties_typeProperties_userName": {
						"value": "[parameters('onpremiseadvetureLT_properties_typeProperties_userName')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
